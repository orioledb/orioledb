CREATE FUNCTION my_eq_p_p(a int, b int) RETURNS bool AS $$
BEGIN
	RETURN my_cmp_plpgsql(a, b) = 0;
END
$$ LANGUAGE plpgsql IMMUTABLE;
CREATE FUNCTION my_eq_p_s(a int, b int) RETURNS bool AS $$
BEGIN
