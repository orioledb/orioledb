CREATE SCHEMA subtransactions;
SET SESSION search_path = 'subtransactions';
CREATE EXTENSION orioledb;
CREATE TABLE o_subtrans (
	id integer NOT NULL,
	value text NOT NULL,
	PRIMARY KEY (id)
) USING orioledb;
